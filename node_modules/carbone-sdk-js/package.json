  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/preset-env": "=7.16.5",
    "core-js": "=3.38.1",
    "esbuild": "=0.23.1",
    "eslint": "=9.10.0",
    "eslint-config-prettier": "=9.1.0",
    "eslint-plugin-jest": "=28.8.3",
    "fetch-mock-jest": "=1.5.1",
    "jest": "=26.6.3",
    "mocha": "=8.4.0",
    "node-fetch": "=2.6.1",
    "prettier": "=3.3.3",
    "regenerator-runtime": "=0.13.7"
  }
